{
  "name": "signalspot",
  "version": "1.0.0",
  "description": "시그널 - 물리적 공간과 디지털 경험을 결합한 소셜 미디어 플랫폼",
  "private": true,
  "workspaces": [
    "backend",
    "frontend"
  ],
  "scripts": {
    "dev": "concurrently \"npm run backend:dev\" \"npm run frontend:dev\"",
    "backend:dev": "cd apps/backend && npm run start:dev",
    "frontend:dev": "cd apps/frontend && npx react-native start",
    "backend:build": "cd apps/backend && npm run build",
    "frontend:build:ios": "cd frontend && npx react-native run-ios --configuration Release",
    "frontend:build:android": "cd frontend && npx react-native run-android --variant=release",
    "test": "nx test",
    "lint": "npm run backend:lint && npm run frontend:lint",
    "backend:lint": "cd apps/backend && npm run lint",
    "frontend:lint": "cd apps/frontend && npm run lint",
    "build": "nx build",
    "e2e": "nx e2e",
    "start": "nx serve",
    "graph": "nx graph",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "changeset publish",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up",
    "docker:dev:build": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml build",
    "docker:dev:down": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml down",
    "docker:metro": "docker-compose --profile mobile-dev up metro",
    "docker:backend": "docker-compose up backend postgres redis",
    "docker:clean": "docker-compose down -v --remove-orphans && docker system prune -f",
    "mobile:android": "cd apps/frontend && npx react-native run-android",
    "mobile:ios": "cd apps/frontend && npx react-native run-ios",
    "mobile:start": "cd apps/frontend && npx react-native start"
  },
